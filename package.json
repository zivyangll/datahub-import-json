{
  "name": "datahub-import-json",
  "version": "0.0.1",
  "description": "",
  "dependencies": {
    "cac": "^6.5.3",
    "node-fetch": "^2.6.0",
    "update-notifier": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^7.5.0",
    "commitlint": "^7.5.2",
    "conventional-changelog-cli": "^2.0.11",
    "eslint": "^5.12.1",
    "eslint-config-antife": "^2.0.0",
    "eslint-config-prettier": "^4.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "mocha": "^5.2.0",
    "np": "^4.0.2",
    "nyc": "^13.1.0",
    "prettier": "^1.16.4"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "test": "nyc mocha",
    "ci": "npm run lint && npm run test",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add .",
    "prepublishOnly": "np --yolo --no-publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zivyangll/datahub-import-json.git"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "bugs": {
    "url": "https://github.com/zivyangll/datahub-import-json/issues"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "homepage": "https://github.com/zivyangll/datahub-import-json#readme",
  "license": "MIT"
}
